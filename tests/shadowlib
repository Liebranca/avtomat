#!/usr/bin/perl

# oh boi

# ---   *   ---   *   ---

  use lib $ENV{'ARPATH'}.'/lib/';
  use lang;

  use v5.36.0;
  use strict;
  use warnings;

  use lib $ENV{'ARPATH'}.'/lib/hacks/';
  use inline;

# ---   *   ---   *   ---

my $s="hi           \n";
print length (lang::stripline($s))."\n";

print lang::is_arrayref([])."\n";
print lang::is_hashref({})."\n";
print lang::is_coderef(sub {})."\n";
print lang::is_qre(qr{magic})."\n";

