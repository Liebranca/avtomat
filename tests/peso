#!/usr/bin/perl

# test peso execution

# ---   *   ---   *   ---
# deps

  use strict;
  use warnings;

  use lib $ENV{'ARPATH'}.'/avtomat/';

  use langdefs::peso;
  use peso::rd;

my $program=peso::rd::parse(

  lang->peso,
  peso::rd->STR,q(

1;2;3;

));

for my$line(@{$program->{cooked}}) {

  printf "$line\n";

};

