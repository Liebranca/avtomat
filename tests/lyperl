#!/usr/bin/perl
#
# Tests my madman filter
#
# ---   *   ---   *   ---

# deps
  use strict;
  use warnings;

  use lib $ENV{'ARPATH'}.'/avtomat/';
  use lyperl;

# ---   *   ---   *   ---

clan base;

# ---   *   ---   *   ---

reg attrs;

  str header 0x00;
  buf body [];

proc base::get;

buf >>:in BEG:
buf >>:out END;

eif out==str;
  ret self->header;

jif out==@;
  ret self->a;

ret null;

# ---   *   ---   *   ---

clan non;

# ---   *   ---   *   ---

my $obj=base::new(v=>2);
print $obj->get."\n";

# ---   *   ---   *   ---
