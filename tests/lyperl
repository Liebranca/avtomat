#!/usr/bin/perl
#
# Tests my madman filter
#
# ---   *   ---   *   ---

# deps
  use strict;
  use warnings;

  use lib $ENV{'ARPATH'}.'/avtomat/';
  use lyperl;

# ---   *   ---   *   ---

class base {

  str xy 'this is a value';

  buf ar 0,1,2;

  obj ob
    name=>'strstr',
    value=>0xFF,

  ;

};

proc base::prich {

  print self->xy."\n";
  print ''.(join ',',@{self->ar})."\n";

  for my $key(keys %{self->ob}) {
    print "$key ".self->ob->{$key}."\n";

  };

};

my $obj=new();
$obj->prich();

# ---   *   ---   *   ---
