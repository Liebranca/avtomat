#!/usr/bin/perl

# Tests caching system

# ---   *   ---   *   ---

# deps
package test;
  use v5.36.0;
  use strict;
  use warnings;

  use lib $ENV{'ARPATH'}.'/lib/';

  use vault 'ARPATH';
  use pricher;

# ---   *   ---   *   ---

my $h={hello=>'goodbye'};
vault::dafwrite('test',$h);

my @blocks=vault::dafread('test',0);
say pricher::fatdump($blocks[0]);

# ---   *   ---   *   ---
1; # ret
