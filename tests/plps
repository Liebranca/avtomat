#!/usr/bin/perl

# tests Peso Language Pattern Syntax

# ---   *   ---   *   ---
# deps
  use strict;
  use warnings;

  use lib $ENV{'ARPATH'}.'/lib/';

  use lang;
  use langdefs::c;

  use peso::rd;
  use avt;

printf lang->c->plps_match(

  'c_func_decl',
  'int* var(static const int** x,const int* y)'

)."\n";

# ---   *   ---   *   ---
1; # ret
