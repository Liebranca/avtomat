#!/usr/bin/perl
# ---   *   ---   *   ---
# THAWOUT
# Loads a serialized Perl object
# from disk and prints it out
#
# LIBRE SOFTWARE
# Licensed under GNU GPL3
# be a bro and inherit
#
# CONTRIBUTORS
# lib,

# ---   *   ---   *   ---
# deps

package thawout;
  use v5.42.0;
  use strict;
  use warnings;

  use Storable qw(retrieve);

  use lib "$ENV{ARPATH}/lib/sys/";
  use Style;
  use Fmat;


# ---   *   ---   *   ---
# the bit

map {
  my $h=retrieve $_ or die strerr $_;
  fatdump \$h,blessed=>1;

} @ARGV;


# ---   *   ---   *   ---
1; # ret
